 {
  "compilerOptions": {
    "module": "es6",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
